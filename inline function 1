#include <iostream>

using namespace std;

inline double square(double n1)
{
    return n1 * n1;
}

inline double Add_a_b(double a1, double b1)
{
    return a1 + b1;
}

inline double Multiply_a_b(double a1, double b1)
{
    return a1 * b1;
}
int main()
{
    double n;
    cout << "Enter Number: " << endl;
    cin >> n;

    cout << "Square of " << n << " is: " << square(n) << endl;
    //cout<<"a1 = 10"<<" "<<" b1 = 20"<<endl;
    cout << "a1 + b1 = " << Add_a_b(10, 20) << endl;
    cout << "a1 * b1 = " << Multiply_a_b(10, 20) << endl;
    return 0;
}
